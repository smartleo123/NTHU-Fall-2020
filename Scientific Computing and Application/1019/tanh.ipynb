   "source": [
    "import numpy as np \n",
    "\n",
    "def f3(x):\n",
    "    return np.tanh(x)\n",
    "\n",
    "def df3(x):\n",
    "    return 1 - np.tanh(x)*np.tanh(x)\n",
    "\n",
    "z = np.linspace(1, 5, 5)\n",
    "z_f3 = f3(z)\n",
    "z_df3 = df3(z)\n",
    "\n",
    "print(z_f3)\n",
    "print(z_df3)\n"
   ]

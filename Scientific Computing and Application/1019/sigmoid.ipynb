   "source": [
    "import numpy as np \n",
    "\n",
    "def f1(x):\n",
    "    return 1/(1 + np.exp(-x))\n",
    "\n",
    "def df1(x):\n",
    "    return f1(x)*(1 - f1(x))\n",
    "\n",
    "z = np.linspace(1, 5, 5)\n",
    "\n",
    "z_f1 = f1(z)\n",
    "z_df1 = df1(z)\n",
    "\n",
    "print(z_f1)\n",
    "print(z_df1)"
   ]
